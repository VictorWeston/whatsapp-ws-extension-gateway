    "TESTING.md",
    "EXTENSION-INTEGRATION.md"
  ],
  "scripts": {
    "test": "node test/gateway.test.js",
    "test:auto": "node run-tests.js",
    "test:server": "node test-server.js",
    "test:client": "node test/mock-client.js",
    "start": "node examples/basic-usage.js",
    "example:basic": "node examples/basic-usage.js",
    "example:express": "node examples/express-middleware.js",
    "example:database": "node examples/with-database.js"
